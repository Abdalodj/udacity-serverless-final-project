{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 2
    },
    "dueDate": {
      "type": "string",
      "pattern": "^d{4}-d{2}-d{2}$"
    }
  },
  "required": ["name", "dueDate"],
  "minLength": 12,
  "additionalProperties": false
}
